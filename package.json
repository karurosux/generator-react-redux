{
  "name": "reatux",
  "version": "0.3.0",
  "description": "Generate react project with redux and foundation lib.",
  "bin": {
    "rtx": "index.js",
    "reatux": "index.js"
  },
  "preferGlobal": true,
  "author": {
    "name": "Carlos Javier Gonzalez Vasquez",
    "email": "justdevelopitmx@gmail.com"
  },
  "files": ["generators", "actions"],
  "main": "index.js",
  "keywords": ["react", "generator", "redux", "foundation", "yeoman-generator"],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.8.0",
    "eslint": "^4.19.1",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.20.1",
    "jest": "^22.0.6"
  },
  "engines": {
    "npm": ">= 4.0.0",
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "change-case": "^3.0.2",
    "commander": "^2.16.0",
    "folder-scanner": "^1.0.0",
    "yeoman-environment": "^2.3.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "karurosux/generator-react-redux-foundation",
  "license": "MIT"
}
